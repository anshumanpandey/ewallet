    "axios": "^0.19.2",
    "axios-hooks": "^2.0.0",
    "formik": "^2.1.4",
    "moment": "^2.27.0",
    "native-base": "^2.13.12",
    "react": "16.11.0",
    "react-hooks-global-state": "^1.0.0",
    "react-native": "0.62.2",
    "react-native-dropdown-picker": "^3.1.10",
    "react-native-file-picker": "0.0.19",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-progress": "^4.1.2",
