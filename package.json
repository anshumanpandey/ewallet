    "react-native-simple-toast": "^1.1.2",
    "react-native-swiper": "^1.6.0",
    "react-navigation": "^4.3.9",
    "react-navigation-drawer": "^2.5.0",
    "react-navigation-stack": "^2.7.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "yup": "^0.29.1"
